{
    "name": "@vafast/static",
    "version": "0.0.6",
    "description": "Plugin for Vafast for static file serving",
    "type": "module",
    "license": "MIT",
    "scripts": {
        "build": "tsdown",
        "dev": "npx tsx watch example/index.ts",
        "test": "npx vitest run",
        "release": "npm run build && npm run test && npx bumpp && npm publish --access=public"
    },
    "peerDependencies": {
        "vafast": "<2.0.0"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "devDependencies": {
        "@types/node": "^24.3.0",
        "rimraf": "^6.0.1",
        "tsdown": "^0.19.0-beta.4",
        "tsx": "^4.20.4",
        "typescript": "^5.4.5",
        "vitest": "^4.0.16"
    },
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        }
    },
    "keywords": [
        "vafast",
        "static",
        "files"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/vafast/vafast-static"
    },
    "author": {
        "name": "saltyAom",
        "url": "https://github.com/SaltyAom",
        "email": "saltyaom@gmail.com"
    },
    "homepage": "https://github.com/vafast/vafast-static",
    "bugs": "https://github.com/vafast/vafast-static/issues",
    "dependencies": {
        "node-cache": "^5.1.2"
    }
}
